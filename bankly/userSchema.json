{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "LouLou",
            "first_name": "Louis",
            "last_name": "Garavaglia",
            "email": "foremail@gmail.com",
            "phone": "555-454-5555",
            "password": "mypassword",
            "admin": true
        }
    ],
    "required": [
        "username",
        "first_name",
        "last_name",
        "email",
        "phone",
        "password"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "LouLou"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Louis"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Garavaglia"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "foremail@gmail.com"
            ]
        },
        "phone": {
            "$id": "#/properties/phone",
            "type": "string",
            "title": "The phone schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "555-454-5555"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "mypassword"
            ]
        },
        "admin": {
            "$id": "#/properties/admin",
            "type": "boolean",
            "title": "The admin schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        }
    },
    "additionalProperties": true
}